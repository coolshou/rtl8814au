PACKAGE_NAME="rtl8814au"
PACKAGE_VERSION="#MODULE_VERSION#"
AUTOINSTALL="YES"
BUILT_MODULE_NAME[0]="8814au"
DEST_MODULE_LOCATION[0]="/updates/dkms"

# Find out how many CPU cores can be use if we pass appropriate -j option to make.
# DKMS could use all cores on multicore systems to build the kernel module.
num_cpu_cores()
{
  if [ -x /usr/bin/nproc ]; then
    nproc
  else
    echo "1"
  fi
}

MAKE="'make' -j$(num_cpu_cores) all KLIB=/lib/modules/$kernelver"
CLEAN="'make' clean"

